[require]
GLSL >= 1.20
GL_ARB_arrays_of_arrays

[vertex shader]
#extension GL_ARB_arrays_of_arrays : enable

uniform int u = 1;
varying vec4 color;

void func(inout int i, inout float f)
{
  i = 0;
  f = 1.;
}

void main()
{
  vec4 v[2][2] = vec4[2][2](vec4[2](vec4(0.), vec4(0.)),
                            vec4[2](vec4(0.), vec4(0.)));
  int i = u;

  func(i, v[1][i][1]);
  color = v[1][1];
  gl_Position = gl_Vertex;
}

[fragment shader]
varying vec4 color;

void main()
{
  gl_FragColor = color;
}

[test]
draw rect -1 -1 2 2
probe all rgb 0. 1. 0.
